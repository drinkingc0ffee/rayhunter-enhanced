{
  "analysis": {
    "time_threshold_seconds": 30,
    "max_correlations": 10000,
    "enable_advanced_parsing": true
  },
  "tools": {
    "scat_path": "/usr/local/bin/scat",
    "enable_scat_integration": true,
    "output_formats": ["json", "csv", "kml"]
  },
  "security": {
    "sanitize_output": true,
    "redact_sensitive_data": true,
    "max_file_size_mb": 100
  },
  "export": {
    "default_output_dir": "./output",
    "include_raw_data": false,
    "generate_reports": true
  },
  "gps": {
    "coordinate_precision": 6,
    "altitude_included": false,
    "time_format": "iso8601"
  }
}
